{%- comment -%}
  Secret Advantage Section
  Fully Dynamic with Complex SVG Support and Responsive Scaling
{%- endcomment -%}

{%- style -%}
  #secret-adv-{{ section.id }} .secret-adv__wrap {
    background: {{ section.settings.bg_color }};
    color: {{ section.settings.text_color }};
    /* Local CSS Variables */
    --heading: {{ section.settings.heading_color }};
    --title: {{ section.settings.subheading_color }};
    --accent: {{ section.settings.accent_color }};
    --text: {{ section.settings.text_color }};
    --white: {{ section.settings.card_title_color }};

    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
    padding-left: 120px;
    padding-right: 120px;
  }

  /* Mobile Image (Hidden on Desktop) */
  #secret-adv-{{ section.id }} .secret-adv__mobile-image { 
    display: none; 
  }

  /* Grid Layout */
  #secret-adv-{{ section.id }} .secret-adv__grid {
    display: grid;
    grid-template-columns: 498px 1fr;
    column-gap: 120px;
    align-items: start;
  }

  /* --- LEFT COLUMN --- */
  #secret-adv-{{ section.id }} .secret-adv__heading {
    margin: 0 0 12px 0;
    color: var(--heading);
    font-size: {{ section.settings.heading_size }}px;
    font-weight: 400;
    line-height: 130%;
    letter-spacing: 1.92px;
    text-transform: capitalize;
  }

  #secret-adv-{{ section.id }} .secret-adv__subheading {
    margin: 0 0 32px 0;
    color: var(--title);
    font-size: {{ section.settings.subheading_size }}px;
    font-weight: 400;
    line-height: 130%;
    letter-spacing: 0.96px;
    text-transform: capitalize;
  }

  #secret-adv-{{ section.id }} .secret-adv__copy p {
    color: var(--text);
    font-size: 16px;
    line-height: 150%;
    letter-spacing: 0.64px;
    margin-bottom: 12px;
  }
  
  #secret-adv-{{ section.id }} .secret-adv__copy h6 {
    color: var(--white);
    font-size: 20px;
    font-weight: 500;
    line-height: 140%;
    letter-spacing: 0.4px;
    text-transform: capitalize;
    margin-top: 12px;
  }

  /* --- RIGHT COLUMN (CARDS) --- */
  #secret-adv-{{ section.id }} .secret-adv__cards {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 64px;
    max-width: 1028px;
  }

  #secret-adv-{{ section.id }} .adv-card {
    display: grid;
    align-self: start;
  }

  #secret-adv-{{ section.id }} .adv-card__icon {
    width: 150px;
    height: 150px;
    color: var(--accent); /* Fallback color for simple SVGs */
    margin-bottom: 0;
  }
  
  #secret-adv-{{ section.id }} .adv-card__icon svg {
    width: 100%;
    height: 100%;
    display: block;
  }

  #secret-adv-{{ section.id }} .adv-card__title {
    margin: 0 0 12px 0;
    color: var(--white);
    font-size: {{ section.settings.card_title_size }}px;
    font-weight: 500;
    line-height: 140%;
    letter-spacing: 0.4px;
    text-transform: capitalize;
    /* Limit width for specific design look */
    max-width: 258px; 
  }

  #secret-adv-{{ section.id }} .adv-card__text {
    color: var(--text);
    font-size: {{ section.settings.card_text_size }}px;
    font-weight: 400;
    line-height: 150%;
    letter-spacing: 0.64px;
  }

  /* --- RESPONSIVE SCALING --- */

  /* Laptop (-10%) */
  @media (min-width: 751px) and (max-width: 1799px) {
    #secret-adv-{{ section.id }} .secret-adv__wrap { padding: calc({{ section.settings.padding_top }}px * 0.9) 108px; }
    #secret-adv-{{ section.id }} .secret-adv__grid { grid-template-columns: 448px 1fr; column-gap: 108px; }

    #secret-adv-{{ section.id }} .secret-adv__heading { font-size: calc({{ section.settings.heading_size }}px * 0.9); letter-spacing: 1.728px; margin-bottom: 10.8px; }
    #secret-adv-{{ section.id }} .secret-adv__subheading { font-size: calc({{ section.settings.subheading_size }}px * 0.9); letter-spacing: 0.864px; margin-bottom: 28.8px; }
    
    #secret-adv-{{ section.id }} .secret-adv__copy p { font-size: 14.4px; letter-spacing: 0.576px; margin-bottom: 10.8px; }
    #secret-adv-{{ section.id }} .secret-adv__copy h6 { font-size: 18px; letter-spacing: 0.36px; }

    #secret-adv-{{ section.id }} .secret-adv__cards { gap: 57.6px; max-width: 925px; }
    #secret-adv-{{ section.id }} .adv-card__icon { width: 135px; height: 135px; }
    #secret-adv-{{ section.id }} .adv-card__title { font-size: calc({{ section.settings.card_title_size }}px * 0.9); letter-spacing: 0.36px; margin-bottom: 10.8px; max-width: 232px; }
    #secret-adv-{{ section.id }} .adv-card__text { font-size: calc({{ section.settings.card_text_size }}px * 0.9); letter-spacing: 0.576px; }
  }

  /* Large Screen (+30%) */
  @media (min-width: 2200px) {
    #secret-adv-{{ section.id }} .secret-adv__wrap { padding: calc({{ section.settings.padding_top }}px * 1.3) 156px; }
    #secret-adv-{{ section.id }} .secret-adv__grid { grid-template-columns: 647px 1fr; column-gap: 156px; }

    #secret-adv-{{ section.id }} .secret-adv__heading { font-size: calc({{ section.settings.heading_size }}px * 1.3); letter-spacing: 2.496px; margin-bottom: 15.6px; }
    #secret-adv-{{ section.id }} .secret-adv__subheading { font-size: calc({{ section.settings.subheading_size }}px * 1.3); letter-spacing: 1.248px; margin-bottom: 41.6px; }

    #secret-adv-{{ section.id }} .secret-adv__copy p { font-size: 20.8px; letter-spacing: 0.832px; margin-bottom: 15.6px; }
    #secret-adv-{{ section.id }} .secret-adv__copy h6 { font-size: 26px; letter-spacing: 0.52px; }

    #secret-adv-{{ section.id }} .secret-adv__cards { gap: 83.2px; max-width: 1336px; }
    #secret-adv-{{ section.id }} .adv-card__icon { width: 195px; height: 195px; }
    #secret-adv-{{ section.id }} .adv-card__title { font-size: calc({{ section.settings.card_title_size }}px * 1.3); letter-spacing: 0.52px; margin-bottom: 15.6px; max-width: 335px; }
    #secret-adv-{{ section.id }} .adv-card__text { font-size: calc({{ section.settings.card_text_size }}px * 1.3); letter-spacing: 0.832px; }
  }

  /* Mobile */
  @media (max-width: 1100px) {
    #secret-adv-{{ section.id }} .secret-adv__grid { grid-template-columns: 1fr; row-gap: 32px; }
    #secret-adv-{{ section.id }} .secret-adv__cards { grid-template-columns: 1fr; gap: 32px; }
  }

  @media (max-width: 749px) {
    #secret-adv-{{ section.id }} .secret-adv__wrap { padding: 80px 16px; }
    
    #secret-adv-{{ section.id }} .secret-adv__mobile-image { 
      display: block; 
      margin: 0 0 32px 0; 
      overflow: hidden; 
      background: #3a3a3a; 
      aspect-ratio: 1/1; 
      border-radius: 12px;
    }
    #secret-adv-{{ section.id }} .secret-adv__mobile-image img { 
      width: 100%; height: 100%; object-fit: cover; display: block; 
    }

    #secret-adv-{{ section.id }} .secret-adv__heading { font-size: 24px; letter-spacing: 0.96px; }
    #secret-adv-{{ section.id }} .secret-adv__subheading { font-size: 20px; letter-spacing: 0.4px; margin-bottom: 16px; }
    
    #secret-adv-{{ section.id }} .secret-adv__copy p { font-size: 14px; letter-spacing: 0.56px; }
    #secret-adv-{{ section.id }} .secret-adv__copy h6 { font-size: 16px; letter-spacing: 0.32px; margin-top: 16px; }

    #secret-adv-{{ section.id }} .adv-card__icon { width: 100px; height: 100px; }
    #secret-adv-{{ section.id }} .adv-card__title { font-size: 16px; letter-spacing: 0.32px; max-width: 100%; }
    #secret-adv-{{ section.id }} .adv-card__text { font-size: 14px; letter-spacing: 0.56px; }
  }
{%- endstyle -%}

<section id="secret-adv-{{ section.id }}" class="secret-adv" aria-label="Titanium Secret Advantage">
  <div class="secret-adv__wrap">
    
    {%- if section.settings.mobile_image != blank -%}
      <div class="secret-adv__mobile-image">
        {{ section.settings.mobile_image | image_url: width: 800 | image_tag: loading: 'lazy', alt: 'Structure' }}
      </div>
    {%- endif -%}

    <div class="secret-adv__grid">
      <div class="secret-adv__left">
        {%- if section.settings.heading != blank -%}
          <h2 class="secret-adv__heading">{{ section.settings.heading }}</h2>
        {%- endif -%}

        {%- if section.settings.subheading != blank -%}
          <h3 class="secret-adv__subheading">{{ section.settings.subheading }}</h3>
        {%- endif -%}

        {%- if section.settings.text != blank -%}
          <div class="secret-adv__copy rte">{{ section.settings.text }}</div>
        {%- endif -%}
      </div>

      <div class="secret-adv__cards">
        {%- for block in section.blocks -%}
          <article class="adv-card" {{ block.shopify_attributes }}>
            <div class="adv-card__icon">
              {%- if block.settings.svg_code != blank -%}
                {{ block.settings.svg_code }}
              {%- else -%}
                <svg viewBox="0 0 151 151" fill="none">
                  <circle cx="75" cy="75" r="50" stroke="currentColor" stroke-width="2.5"/>
                  <path d="M75 40V110M40 75H110" stroke="currentColor" stroke-width="2.5"/>
                </svg>
              {%- endif -%}
            </div>
            
            <h4 class="adv-card__title">{{ block.settings.title }}</h4>
            <div class="adv-card__text rte">{{ block.settings.text }}</div>
          </article>
        {%- endfor -%}
      </div>
    </div>
  </div>
</section>

